{
    "type": "robo-tasks",
    "name": "sweetchuck/robo-yarn",
    "description": "Robo task wrappers for Yarn commands",
    "keywords": [
        "robo-tasks",
        "yarn"
    ],
    "authors": [
        {
            "name": "Andor",
            "role": "Maintainer",
            "homepage": "https://github.com/Sweetchuck"
        }
    ],
    "support": {
        "source": "https://github.com/Sweetchuck/robo-yarn",
        "issues": "https://github.com/Sweetchuck/robo-yarn/issues"
    },
    "license": "GPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "sweetchuck/git-hooks": true
        },
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "repositories": {},
    "require": {
        "php": ">=8.2",
        "ext-json": "*",
        "consolidation/robo": "^4.0",
        "mindscreen/yarnlock": "^1.0",
        "sweetchuck/utils": "2.x-dev"
    },
    "require-dev": {
        "codeception/codeception": "^5.0",
        "codeception/module-asserts": "^3.0",
        "mikey179/vfsstream": "^1.6",
        "nuvoleweb/robo-config": "^3.0",
        "squizlabs/php_codesniffer": "^3.5",
        "sweetchuck/codeception-module-robo-task-runner": "3.x-dev",
        "sweetchuck/git-hooks": "2.x-dev",
        "sweetchuck/robo-git": "3.x-dev",
        "sweetchuck/robo-phpcs": "3.x-dev",
        "symfony/error-handler": "^5.0 || ^6.0",
        "symfony/filesystem": "^5.4 || ^6.0",
        "symfony/finder": "^5.0 || ^6.0"
    },
    "autoload": {
        "psr-4": {
            "Sweetchuck\\Robo\\Yarn\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sweetchuck\\Robo\\Yarn\\Tests\\Acceptance\\": "tests/acceptance/",
            "Sweetchuck\\Robo\\Yarn\\Tests\\Unit\\": "tests/unit/",
            "Sweetchuck\\Robo\\Yarn\\Tests\\": "tests/_support/"
        }
    },
    "scripts": {},
    "scripts-descriptions": {}
}
